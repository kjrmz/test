-- Functions
local function animateButton(button, scale)
    local tween =
        TweenService:Create(
        button,
        TweenInfo.new(0.15, Enum.EasingStyle.Quad),
        {
            Size = UDim2.new(button.Size.X.Scale, button.Size.X.Offset, 0, scale)
        }
    )
    tween:Play()
end

local function loadScript(url, platform)
    StatusLabel.Text = "Loading " .. platform .. " script..."
    StatusIcon.Text = ">"
    StatusIcon.TextColor3 = Color3.fromRGB(255, 200, 100)

    local success, result =
        pcall(
        function()
            local scriptContent = game:HttpGet(url)
            loadstring(scriptContent)()
        end
    )

    if success then
        StatusLabel.Text = platform .. " script loaded successfully!"
        StatusIcon.Text = "+"
        StatusIcon.TextColor3 = Color3.fromRGB(100, 255, 100)
        task.wait(1.5)
        ScreenGui:Destroy()
    else
        StatusLabel.Text = "Failed to load script - Check connection"
        StatusIcon.Text = "X"
        StatusIcon.TextColor3 = Color3.fromRGB(255, 100, 100)
        task.wait(2)
        StatusLabel.Text = "Ready to load"
        StatusIcon.Text = "O"
        StatusIcon.TextColor3 = Color3.fromRGB(100, 255, 100)
    end
end

-- Hover Effects
PCButton.MouseEnter:Connect(
    function()
        PCButton.BackgroundColor3 = Color3.fromRGB(90, 150, 255)
        animateButton(PCButton, 38)
        PCStroke.Transparency = 0.3
    end
)

PCButton.MouseLeave:Connect(
    function()
        PCButton.BackgroundColor3 = Color3.fromRGB(70, 130, 255)
        animateButton(PCButton, 35)
        PCStroke.Transparency = 0.7
    end
)

MobileButton.MouseEnter:Connect(
    function()
        MobileButton.BackgroundColor3 = Color3.fromRGB(170, 90, 255)
        animateButton(MobileButton, 38)
        MobileStroke.Transparency = 0.3
    end
)

MobileButton.MouseLeave:Connect(
    function()
        MobileButton.BackgroundColor3 = Color3.fromRGB(150, 70, 255)
        animateButton(MobileButton, 35)
        MobileStroke.Transparency = 0.7
    end
)

CloseButton.MouseEnter:Connect(
    function()
        CloseButton.BackgroundColor3 = Color3.fromRGB(255, 90, 90)
    end
)

CloseButton.MouseLeave:Connect(
    function()
        CloseButton.BackgroundColor3 = Color3.fromRGB(255, 70, 70)
    end
)

-- Button Clicks
PCButton.MouseButton1Click:Connect(
    function()
        loadScript(PC_SCRIPT, "PC")
    end
)

MobileButton.MouseButton1Click:Connect(
    function()
        loadScript(MOBILE_SCRIPT, "Mobile")
    end
)

CloseButton.MouseButton1Click:Connect(
    function()
        ScreenGui:Destroy()
    end
)
